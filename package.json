{
  "name": "sinergia",
  "version": "1.0.0",
  "description": "Sinergia Plus, Company web-site",
  "main": "./src/index.js",
  "homepage": "https://nikolaycviv.github.io/Sinergia/",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "concurrently \"npm run react-start\" \"npm run sass\"",
